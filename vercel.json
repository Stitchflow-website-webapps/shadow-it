{
  "functions": {
    "app/api/auth/google/route.ts": {
      "maxDuration": 300
    },
    "app/api/background/sync/route.ts": {
      "maxDuration": 300,
      "memory": 1024
    },
    "app/api/sync/status/route.ts": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "regions": ["iad1"],
  "crons": []
} 